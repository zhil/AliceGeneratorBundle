{
    "name": "trappar/alice-generator-bundle",
    "description": "Symfony bundle for generating Alice fixtures from Doctrine entities",
    "keywords": ["fixture", "data", "test", "orm", "doctrine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Way",
            "email": "jeff.way@me.com"
        }
    ],
    "require": {
        "php": "^5.6||^7.0",
        "trappar/alice-generator": "dev-issue_10",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/finder": "^2.5|~3.0",
        "symfony/validator": "~2.5|~3.0",
        "symfony/console": "~2.5|~3.0",
        "symfony/framework-bundle": "~2.5|~3.0",
        "symfony/yaml": "~2.5|~3.0",
        "symfony/phpunit-bridge": "^2.7.4|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Trappar\\AliceGeneratorBundle\\": "src/Trappar/AliceGeneratorBundle" }
    }
}
